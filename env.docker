# Docker Environment Variables
# Used when running in Docker containers

# API Base URLs - Docker internal networking

REACT_APP_API_BASE_URL=http://fastapi-gateway:3001
REACT_APP_MSTR_API_URL=http://mstr-dax-api:8001
REACT_APP_COGNOS_API_URL=http://cognos-dax-api:8003
REACT_APP_TABLEAU_API_URL=http://tableau-dax-api:8004


# Docker Mode
REACT_APP_ENV=docker

# Enable debug logging for Docker troubleshooting
REACT_APP_DEBUG=true

# Docker API timeout
REACT_APP_API_TIMEOUT=30000
