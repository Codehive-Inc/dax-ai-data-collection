# Production Environment Variables
# Copy this file to .env.production for production builds

# API Base URLs - Production
REACT_APP_API_BASE_URL=http://localhost:3001
REACT_APP_MSTR_API_URL=http://localhost:8080
REACT_APP_COGNOS_API_URL=http://localhost:8001
REACT_APP_TABLEAU_API_URL=http://localhost:8003

# Production Mode
REACT_APP_ENV=production

# Disable debug logging in production
REACT_APP_DEBUG=false

# Production API timeout (shorter for better UX)
REACT_APP_API_TIMEOUT=15000
