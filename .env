# Environment Variables for Docker Deployment with Nginx Proxy
# Copy this to .env.data-collection for Docker deployment

# API Endpoints (using Nginx proxy paths for Docker)
REACT_APP_MSTR_API_URL=/api/mstr
REACT_APP_COGNOS_API_URL=/api/cognos
REACT_APP_TABLEAU_API_URL=/api/tableau

# For local development, use:
# REACT_APP_MSTR_API_URL=http://localhost:8001
# REACT_APP_COGNOS_API_URL=http://localhost:8003
# REACT_APP_TABLEAU_API_URL=http://localhost:8004

# Configuration
REACT_APP_DEBUG=true
REACT_APP_API_TIMEOUT=30000
REACT_APP_ENV=production
