# React App Environment Variables
# Copy this file to .env and update the values as needed
# These variables must be prefixed with REACT_APP_ to be accessible in React

# API Base URLs - Using nginx proxy for internal Docker communication
# All API calls go through the frontend nginx proxy on port 3000
REACT_APP_API_BASE_URL=/api/mstr
REACT_APP_MSTR_API_URL=/api/mstr
REACT_APP_COGNOS_API_URL=/api/cognos
REACT_APP_TABLEAU_API_URL=/api/tableau

# Development/Production Mode
REACT_APP_ENV=production

# Optional: Enable debug logging
REACT_APP_DEBUG=true

# Optional: API timeout in milliseconds
REACT_APP_API_TIMEOUT=30000
